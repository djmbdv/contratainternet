            });
            $(".guia-ref").css({
                position: 'fixed'
            });
        }
    }

    if ($(".fixedt").length != 0) {
        if ($(window).scrollTop() > $('.statickt').offset().top) {
            $('.fixedt').css('opacity', '1');
            $('.gotup').css('opacity', '1');
        } else {
            $('.fixedt').css('opacity', '0');
            $('.gotup').css('opacity', '0');
        }

        if (isScrolledIntoView(".fl2")) {
            $('.fixedt').css('opacity', '0');
            $('.gotup').css('opacity', '0');
        }
    }
})

function isScrolledIntoView(elem) {
    var docViewTop = $(window).scrollTop();
    var docViewBottom = docViewTop + $(window).height();
    var elemTop = $(elem).offset().top;
    return ((elemTop <= docViewBottom) && (elemTop >= docViewTop));
}

function contadorCanales() {

    $(".categ-guia").each(function () {

        var canalsCont = $(this);
        var catCanal = $(".categ-guia").index(this);
        var cantCanals = $(canalsCont).find(".canal").length;
        var cont = 0;

        $(canalsCont).find(".canal").each(function () {
            var thisCanal = $(this);
            if ($(thisCanal).is(":visible")) {
                cont++;
            }
        });

        if (cont <= 0) {
            $(canalsCont).find("h2").css("display", "none");
            $(canalsCont).find("h3").css("display", "none");
        } else {
            $(canalsCont).find("h2").css("display", "block");
            $(canalsCont).find("h3").css("display", "block");
        }


    })

}

