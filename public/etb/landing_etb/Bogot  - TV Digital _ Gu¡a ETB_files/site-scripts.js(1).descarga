            }
            break;
        }
    }
    if (d.getDay() == 0) {
        if (((d.getHours()) > 7) && ((d.getHours()) < 18)) {
            window.location = "https://etbcolombia.custhelp.com/app/chat/chat_launch"; //direcciona
        } else {
            window.location = "http://www.etb.com.co/chatventashog/"; //direcciona
