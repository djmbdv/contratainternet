function Boxlide() {

    var tam22 = ($(window).width() - 100) + 'px';
    var tam23 = ($(window).height() - 100) + 'px';
    $('.secb').css('height', tam23);
    $('.bl-content').css('width', tam22);

    $('#blslid').not('.slick-initialized').slick({
        infinite: true,
        slidesToShow: 3,
        slidesToScroll: 3,
        autoplay: false,
        autoplaySpeed: 8000,
        adaptiveHeight: true,
        arrows: true,
        dots: false,
        pauseOnHover: true
    });    

    $('.secb').click(function () {        
        if (!($(this).hasClass('isExpanded'))) {
            $('.slick-track').css('transform', '');
            $(this).css('z-index', '8');
            $(this).addClass('isExpanded');
            $(this).find('.bl-content').addClass('bl-show');
        } else {
            $(this).css('z-index', '6');
            $(this).removeClass('isExpanded');
            $('.bl-content').removeClass('bl-show');
        }
    });

    $('#blslid .slick-arrow').click(function () {
        $('.secb').css('z-index', '6');
        $('.secb').removeClass('isExpanded');
        $('.bl-content').removeClass('bl-show');
    });
};

function eventClick() {
    if (!($(this).hasClass('isExpanded'))) {
        $('.slick-track').css('transform', '');
        $(this).css('z-index', '8');
        $(this).addClass('isExpanded');
        $(this).find('.bl-content').addClass('bl-show');
    }
}

function getItemLayoutProp(items) {

    var scrollT = $(window).scrollTop(),
        scrollL = $(window).scrollLeft(),
        itemOffset = $(items).offset();

    return {
        left: itemOffset.left - scrollL,
        top: itemOffset.top - scrollT,
        width: $(items).outerWidth(),
        height: $(items).outerHeight()
    };
};

function getWindowSize() {
    var w = $(".bl-even").width();
    var h = $(".bl-even").height();
    return {
        width: w,
        height: h
    };
};

