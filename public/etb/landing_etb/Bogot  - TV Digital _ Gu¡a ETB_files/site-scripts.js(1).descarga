function OpenLeftData() {
    $(".masdata").click(function () {
        if (!$(this).hasClass('masdOpen')) {
            $(this).addClass('masdOpen');
            $(".data-estleft").animate({
                height: "80%"
            }, 400, function () {
                $(".data-estleft").css('overflowY', 'auto');
            });
        } else {
            $(this).removeClass('masdOpen');
            $(".data-estleft").animate({
                height: "100px"
            }, 400, function () {
                $(".data-estleft").css('overflowY', 'hidden');
            });
        }
