    //$(".content-canales").on("scroll", onScroll);
};

function LeScroll(element, parent) {
    $(parent)[0].scrollIntoView(true);
    $(parent).animate({
        scrollTop: ($(parent).scrollTop() + $(element).offset().top - $(parent).offset().top) - 62
    }, {
            duration: 'slow',
            easing: 'swing'
        });
}

