function horariochat() {
    var d = new Date();
    var festivos = ['20/6/2016', '7/7/2016', '15/7/2016', '17/9/2016', '7/10/2016', '14/10/2016', '8/11/2016', '25/11/2016', '1/0/2017', '9/0/2017', '20/2/2017', '13/3/2017', '14/3/2017', '16/3/2017', '1/4/2017', '26/4/2017', '19/5/2017', '26/5/2017', '3/6/2017', '20/6/2017', '7/7/2017', '21/7/2017', '16/9/2017', '6/10/2017', '13/10/2017', '8/11/2017', '25/11/2017']; //festivos
    var e = (d.getDate() + "/" + d.getMonth() + "/" + d.getFullYear()); //fecha actual
    //calcula festivos
    for (var x = 0; x < festivos.length; x++) {
        if (festivos[x] == e) {
            if (((d.getHours()) > 7) && ((d.getHours()) < 18)) {
                window.location = "https://www.facebook.com/ETB.Colombia/"; //direcciona
            } else {
